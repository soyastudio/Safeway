					<xs:documentation>Indicated if this is the identifying address for the party</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PrimaryInd" minOccurs="0">
				<xs:annotation>
