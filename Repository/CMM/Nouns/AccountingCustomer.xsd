<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://collab.safeway.com/it/architecture/info/default.aspx" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:Abs="http://collab.safeway.com/it/architecture/info/default.aspx" targetNamespace="http://collab.safeway.com/it/architecture/info/default.aspx" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--V1.0  Patrick Oliver 7/13/20 - Initial Draft-->
	<xs:include schemaLocation="../Nouns/FinanceMaster.xsd"/>
	<xs:include schemaLocation="../AbsMaster/AbsOmMaster.xsd"/>
	<xs:complexType name="AccountingCustomer">
		<xs:annotation>
			<xs:documentation>The Accounting Customer object will contain customer information that is relevant to a specific company level customer. Customer processing rules, statistical fields and other processing fields are part of the object. In addition to keep customer information synchronized from legacy to new ERP the object will allow for Customer Groups/Customers and Customer Parties/Accounts.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="CompanyId" minOccurs="0"/>
			<xs:element name="CustomerGroup" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Legacy application customer groups.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CustomerGroupId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Customer group. A group that customers belong to that transcends companies.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="10"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="CustomerGroupNm" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The name or description of the customer group.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="50"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="AutoNbr" type="CodeWithDescription" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Automatic customer numbering option. This option can be used only if the numeric customer number option was  specified. Valid values are: N = Manual customer numbering Y = Automatic customer numbering</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="AgeData" minOccurs="0">
							<xs:annotation>
								<xs:documentation>.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="AgeCredits" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Determines aging rules for credits for cross-company agings.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="AgePymnt" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Determines aging rules for payments for cross-company agings.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="AgeDisputes" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Aging default for disputes when the aging is across companies.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="AgeType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Default aging method when aging cross companies.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="AgeCurrentNbr" type="xs:integer" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Used to define the transactions going into the "current" and "future" aging
buckets.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="AgePeriodsNbr" type="xs:integer" minOccurs="0" maxOccurs="4">
										<xs:annotation>
											<xs:documentation>Aging periods are user defined and established by entering the last day of                        each aging bucket in the Age Period                     field.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="BillofExchangeProcessInd" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Bill of exchange processing indicator which indicate whether this customer group processes
bills of exchange. Valid values are:
N = No; don't process bills of exchange
Y = Yes; process bills of exchange</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="EffectivePeriod" type="EffetiveDateTimeType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="CustomerParty" type="CustomerPartyType">
				<xs:annotation>
					<xs:documentation>The customer party can be a Person, Organization, Group or Relationship. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CustomerPartySite" type="CustomerPartySitesType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="CustomerSource" type="SourceSystemType" maxOccurs="unbounded"/>
			<xs:element name="CustomerAccounts" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CustomerAccount" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="CustomerAccountNbr" minOccurs="0"/>
									<xs:element name="CustomerSrcNbr" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="CustomerSiteNbr" minOccurs="0">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="AccountTypeCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates the type of customer account</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="KeyAccountInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Key account for sales representatives.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="AccountClassId" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Customer class indentifier.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="AccountDsc" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Description chosen by external party </xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="250"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="EffectivePeriod" type="EffetiveDateTimeType" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AccountSource" type="SourceSystemType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="AccountPurpose" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="PurposeNbr" type="xs:decimal" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Business purpose number</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="PurposeInd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Business purpose assigned to customer site account, such as Bill_To, Market, and Statements.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="PrimaryInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates if this site purpose is the primary site purpose for this customer account site. Y for the primary customer account site purpose. N for non-primary.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="EffectivePeriod" type="EffetiveDateTimeType" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AccountRelationship" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="RelationshipNbr" type="xs:integer" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Number used to identify a set of reference data shared across business units and other entities</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="RelationshipTypeCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates the type of customer account</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ReciprocalInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates if this relationship is reciprocal or not. Y for reciprocal relationships, N for relationships that are not reciprocal.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="BillToInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates if this relationshiop is bill-to or not.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="ShipToInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates if this relationship is ship-to or not.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="EffectivePeriod" type="EffetiveDateTimeType" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AccountClassification" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="CategoryClsCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Classification code Classification category name</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="PrimaryClsInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates that the classification is primary.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="RankClsNbr" type="xs:integer" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Used to rank classes if multiple classes have been assigned to a party.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="EffectivePeriod" type="EffetiveDateTimeType" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AccountStatus" minOccurs="0">
							<xs:complexType>
								<xs:sequence minOccurs="0">
									<xs:element name="StatusCd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Status code for activation and/or reactivation.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="10"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="StatusDsc" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Description of the status code for activation and/or reactivation.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="StatusVal" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Value of the status.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="SourceNm" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Name or description of the source.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="ChannelCd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Code of the channel type.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:integer">
												<xs:totalDigits value="3"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="PlatformCd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Code of the platform type.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:integer">
												<xs:totalDigits value="3"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="EffectivePeriod" type="EffetiveDateTimeType" minOccurs="0"/>
								</xs:sequence>
								<xs:attribute ref="typeCode"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="AccountAddresses" type="AccoutingCustomerAddressType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="AccountContact" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Represents communication details including address, voice, fax, and digital channels.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="CommunicationType"/>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="AccountRole" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Customer contact can perform multiple roles that have different codes.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="DepartmentId" minOccurs="0"/>
									<xs:element name="DepartmentCd" type="CodeWithDescription" minOccurs="0"/>
									<xs:element name="RoleTypeCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Type of role a contact person performs.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="PrimaryInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates if this person is the primary contact for this account. Y for yes, the person is the primary contact for this account, N for non-primary.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="5"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="RoleLevelInd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Role level of the contact. For example, executive, department head, or middle manager. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="PrimaryRoleInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates if this is the primary role for this contact. Y for primary, N for a non-primary role for the contact. The default value is N. </xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="5"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="RoleTypePrimaryInd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates if the contact is the primary contact for this role. Y for the primary contact for this role type, N for a contact that is not the primary contact for this role type. The default value is N. </xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="5"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="RoleTitle" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Free form text for job title.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="100"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AccountProfile" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Represensts the customer account profile</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ProfileClsCd" type="CodeWithDescription" minOccurs="0"/>
									<xs:element name="CreditReviewCycleCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Credit review cycle that determines the frequency of periodic credit reviews</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="OrderLimitNbr" type="xs:decimal" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Order Limit in customer group currency for a given customer across all AR companies utilizing this customer.  </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CreditLimitNbr" type="xs:decimal" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Maximum amount of credit extended to a customer, in customer group currency, for all AR compaies utilizing this customer.        </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CreditToleranceNbr" type="xs:integer" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Credit check tolerance amount over the credit limit.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CreditRatingNbr" type="xs:integer" minOccurs="0">
										<xs:annotation>
											<xs:documentation>credit rating of the customer account or site.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="DiscountRatePct" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The rate used to calculate the discount amount of the invoice payment.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:decimal">
												<xs:totalDigits value="5"/>
												<xs:fractionDigits value="3"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="DiscountDt" type="DateType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The date through which the available discount amount of the invoice can be taken.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CreditLimitDt" type="xs:date" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The date the credit limit was defined or last reviewed.     </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="LastReviewDt" type="xs:date" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Last Review Date   </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="NextReviewDt" type="xs:date" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Review Date   </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="BillTypeCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Format of balance forward bill. Can be SUMMARY, DETAIL, or IMPORTED (for Site only).</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ShipToSiteNbr" type="xs:integer" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>This will be used to store customer ship to numbers </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="BillToSiteNbr" type="xs:integer" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>This will be used to store customer bill to numbers</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="BillLevelCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Level at which balance forward billing is done, ACCOUNT or SITE. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="NationalAccntInd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates whether the customer is a national account customer. N = Parent and I = Invoiced</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CustomerHoldCd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Indicates whether a customer is on hold</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="4"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="CreditAnalystCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>tThe credit analyst code assigned to this customer.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CollectorCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Collector name</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="SalesRepresentativeCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The sales representative for this  invoice, debit memo or credit memo (note numeric)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="DunProcessLetterCd" type="CodeWithDescription" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Dunning letter process code.  A unique code that represents a combination of Dunning Letter options.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="PrePaymentCd" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Prepayment code. This is the default code to be used for a check entered as a preayment. This will be the offset instead of RNA to cash.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="20"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element ref="AccountReceivableCd" minOccurs="0"/>
									<xs:element name="AutoCashNm" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Autocash hierarchy name.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="50"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="CurrencyCd" minOccurs="0">
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="CodeWithDescription">
													<xs:sequence minOccurs="0">
														<xs:element ref="CurrencyExchangeRate" minOccurs="0"/>
													</xs:sequence>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="Overrides" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Various kinds of overrides applied to customer account</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="SendCreditBalanceInd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates if statement with credit balance can be sent to customer.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="IncludeCreditCheckInd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates whether a customer account is included in credit checking.  Y for inclusion in credit checking, N for optional credit checking.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="CreditHoldInd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates whether a customer account is placed on credit hold.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="AllowDiscountnd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates if discount terms are allowed. Y for discount terms allowed, N for no discount terms.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="SendDunningLetternd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates whether to send dunning letter to a customer account or site.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="EnableLateChargeInd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates if late charges are enabled. Y for enabling late charges, N for not enable late charges.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="SendStatementInd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates whether to create statements for a customer account or site.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="AutoReceiptsDisputeInd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates whether to include disputed transaction in automatic receipt. Y for including disputed transactions, N for not excluding disputed transaction</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="BalanceForwardBillingInd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates if balance forward bill is generated and sent to customer. Y for yes, N or null for no.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="OverrideTermsInd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates whether to allow users to override standard terms when creating invoices.</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="AutoCashInd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Indicates the automatic cash application method for this customer. </xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="AutoRemoveInd" minOccurs="0">
													<xs:annotation>
														<xs:documentation>This field determines whether payments and trransactions can be removed automatically (Y/N)</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:maxLength value="1"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AccountAmounts" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="CurrentBalanceAmt" type="AmountType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>This is the current, non Bill of      Exchange balance for this customer,  cumulative for all companies utilizing this customer, in customer group currency.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="DraftBalanceAmt" type="AmountType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>This is the current Bill of Exchange balance for this customer, cumulative                        for all companies utilizing this          customer, in customer group  currency.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="MinimumReceiptAmt" type="AmountType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Minimum receipt amount for automatic receipts.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="MinDunningAmt" type="AmountType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Minimum amount for the customer account or site to send dunning letter.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="MinBalanceOverdueAmt" type="AmountType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Minimum balance required for late charge calculation.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="MinStatementAmt" type="AmountType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Minimum statement amount for the customer account or site to send statements.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="LastPymtAmt" type="AmountType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The amount of the last payment made by this customer.  </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="EffectivePeriod" type="EffetiveDateTimeType" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AccountBankData" type="BankType" minOccurs="0"/>
						<xs:element name="LineTax" type="TaxType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="PaymentTerms" type="PaymentTermType" minOccurs="0" maxOccurs="5">
							<xs:annotation>
								<xs:documentation>Identifies the payment term for customer</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CorpCardType">
		<xs:annotation>
			<xs:documentation>Corp Card type will contain card information for cards submitted to customers (ex: schools, churches, etc.). The card information will be card name, card number, expiration dates,  pin number, amounts, etc. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CardNbr" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>It is the number of the corp card </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecordNbr" type="xs:integer" minOccurs="0"/>
			<xs:element name="Amount" type="AmountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is the numerical value of the amount for transaction to or from the cardholder?s credit card account. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LastUpdateTs" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Timestamp for last update</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CardStatus" type="StatusType" minOccurs="0"/>
			<xs:element name="CardNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Card name</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CustomerNm" type="PersonNameType" minOccurs="0"/>
			<xs:element name="PinNbr" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Personal identification number (PIN) is a numerical code issued with a payment card that is required to be entered to complete various financial transactions. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExpirationDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>MMYY for when the cardholder?s credit card expires.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CardCd" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Thie card code, also called CVC, is usually between 3 and 6 characters long and will vary depending on the type of credit card. For VISA and Mastercard the CVC is often a 3 digit code printed on the reverse side of the card. American express often print a 4 digit code on the front side of the card. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Merchant" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="MerchantId" type="CodeWithDescription" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This is your merchant services account identifier.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MerchantNm" minOccurs="0">
							<xs:annotation>
								<xs:documentation>ex: PAYPAL INC MARK</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="200"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="MerchantAddress" type="AddressType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="AmountType" minOccurs="0"/>
			<xs:element name="BillingAddress" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The billing address for purposes of address verification when processing a credit card</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShippingAddress" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The shipping address for payment transactions</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ImageRequiredInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicator is say if image is needed for credit card expenses. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BilledDescription" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomerVendor">
		<xs:annotation>
			<xs:documentation>Contains vendor information related to AR Customers</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="VendorId" minOccurs="0"/>
			<xs:element name="VendorSubAccountId" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="VendorGrp" minOccurs="0"/>
			<xs:element ref="VendorClass" minOccurs="0"/>
			<xs:element ref="VendorClassDsc" minOccurs="0"/>
			<xs:element name="VendorNm" type="xs:string" minOccurs="0"/>
			<xs:element name="VendorContact" minOccurs="0" maxOccurs="2">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ContactNm" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="50"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="PhoneNbr" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="20"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="EmailAddresstxt" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="100"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Address" type="AddressType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomerGrpType">
		<xs:annotation>
			<xs:documentation>The AR Customer Group file contains customer group records. A customer group
defines the customer number/numbering conventions, and allows one or more
companies to use the same set of customers. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="CompanyId"/>
			<xs:element ref="CustomerAccountNbr"/>
			<xs:element ref="AccountReceivableCd" minOccurs="0"/>
			<xs:element name="ARCustomerStatus" type="StatusType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the status of the customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrePaymentCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Prepayment code. This is the default code to be used for a check entered as a preayment. This will be the offset instead of RNA to cash.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NationalAccntInd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the customer is a national account customer. N = Parent and I = Invoiced</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ARCustomerBalances" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CurrentBalance" type="ARBalanceType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This is the current, non Bill of      Exchange balance for this customer,  cumulative for all companies utilizing this customer, in customer group currency.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DraftBalance" type="ARBalanceType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This is the current Bill of Exchange balance for this customer, cumulative                        for all companies utilizing this          customer, in customer group  currency.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ARCustomerLimits" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="OrderLimit" type="xs:decimal" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Order Limit in customer group currency for a given customer across all AR companies utilizing this customer.  </xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CreditLimit" type="xs:decimal" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Maximum amount of credit extended to a customer, in customer group currency, for all AR compaies utilizing this customer.        </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ARCustomerHoldCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer Hold Code.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ARSalesRepresentativeCd" type="CodeWithDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The sales representative for this  invoice, debit memo or credit memo (note numeric)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AutoRemoveInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field determines whether payments and trransactions can be removed automatically (Y/N)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AutoCashInd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the automatic cash application method for this customer. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ARCustomerStartDt" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date the customer first began doing business with the company.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineTax" type="TaxType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PaymentTerms" type="PaymentTermType" minOccurs="0" maxOccurs="5">
				<xs:annotation>
					<xs:documentation>Identifies the payment term for customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ARCustomerUser" minOccurs="0" maxOccurs="10">
				<xs:annotation>
					<xs:documentation>Customer user-defined field</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomerPartySitesType">
		<xs:annotation>
			<xs:documentation>Object used to identify various paty sites</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PartySiteId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Generic identifier field for party site. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PartySiteNm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name of the Party Site.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="250"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MailStopCd" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A user-defined code to indicate a mail drop point within their organization.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
